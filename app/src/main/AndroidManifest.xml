<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.materdesigndemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Test"/>
        <activity android:name=".activity.ToolbarActivity"/>
        <activity android:name=".activity.CardViewActivity"/>
        <activity android:name=".activity.BaseActivity"/>
        <activity android:name=".activity.PaletteActivity"/>
        <activity android:name=".activity.XiTuActivity"/>
    </application>

</manifest>